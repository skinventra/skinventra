{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**", ".vite/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:tunnel": {
      "cache": false,
      "persistent": true
    },
    "dev:prod": {
      "cache": false,
      "persistent": true
    },
    "preview": {
      "cache": false,
      "persistent": true
    },
    "start:prod": {
      "cache": false,
      "persistent": true
    },
    "start:tunnel": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "prisma:generate": {
      "cache": false
    },
    "prisma:migrate": {
      "cache": false
    },
    "prisma:studio": {
      "cache": false,
      "persistent": true
    }
  }
}

